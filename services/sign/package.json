{
  "name": "@calibur/sign",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=production vue-cli-service serve",
    "build": "cross-env RUN_TYPE=build vue-cli-service build --modern",
    "upload": "ossutil cp -r ./dist oss://calibur-arthur/sign/ -f",
    "deploy": "npm run build && npm run upload"
  },
  "dependencies": {
    "@arthur/shared-css": "workspace:shared/css",
    "@calibur/bus": "workspace:modules/bus",
    "@calibur/component": "workspace:modules/component",
    "@calibur/http": "workspace:modules/http",
    "@calibur/user": "workspace:modules/user",
    "core-js": "^3.10.1",
    "element-plus": "^1.0.2-beta.40",
    "js-cookie": "^2.2.1",
    "vue": "^3.0.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.12",
    "@vue/cli-service": "~4.5.12",
    "@vue/compiler-sfc": "^3.0.11",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^7.0.3"
  }
}
