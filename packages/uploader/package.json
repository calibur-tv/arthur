{
  "name": "uploader",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=production vue-cli-service serve",
    "build": "cross-env RUN_TYPE=build vue-cli-service build --modern",
    "upload": "ossutil cp -r ./dist oss://calibur-arthur/uploader/ -f",
    "deploy": "npm run build && npm run upload"
  },
  "dependencies": {
    "filepond": "^4.25.1",
    "filepond-plugin-file-rename": "^1.1.6",
    "filepond-plugin-file-validate-size": "^2.2.2",
    "filepond-plugin-file-validate-type": "^1.2.5",
    "filepond-plugin-image-exif-orientation": "^1.0.9",
    "filepond-plugin-image-preview": "^4.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "cross-env": "^7.0.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
